{
  "bitwise": false,
  "esversion": 6,
  "curly": false,
  "eqeqeq": true,
  "freeze": true,
  "latedef": "nofunc",
  "maxparams": 7,
  "noarg": true,
  "shadow": "inner",
  "undef": true,
  "unused": "vars",

  "boss": true,
  "expr": true,
  "eqnull": true,
  "evil": true,
  "loopfunc": true,
  "proto": true,
  "supernew": true,

  "-W018": true,
  "-W064": true,
  "-W086": true,
  "-W032": true,
  "-W021": true,

  "browser": true,
  "browserify": true,
  "node": true,
  "nonstandard": true,
  "typed": true,
  "worker": false,

  "camelcase": false,
  "indent": 2,
  "maxlen": 110,
  "newcap": false,
  "quotmark": "single",

  "laxbreak": true,
  "laxcomma": true,

  "noyield": true,

  "predef": [
    "it",
    "describe",
    "beforeEach"
  ]
}
